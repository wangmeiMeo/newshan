<template name="index$75d4da51">
  <view class="_div data-v-8e3aa6a4">
    <view class="_div data-v-8e3aa6a4 coupon">
      <view wx:if="{{coupons=='undefined' || coupons.length==0}}" class="_div data-v-8e3aa6a4">
        <image class="_img data-v-8e3aa6a4 couponicon" src="{{iconconpon}}" alt></image>
        <label class="_span data-v-8e3aa6a4 sp1">^_^没有更多优惠券o_o</label>
      </view>
      <view class="_view data-v-8e3aa6a4 coupons" wx:else>
        <view class="_view data-v-8e3aa6a4 coupon-lists" wx:key="index" key="{{index}}" wx:for="{{coupons}}" wx:for-index="index" wx:for-item="item">
          <image src="/copy-asset/static/images/logos1.png" class="_img data-v-8e3aa6a4 coupon-logos"></image>
          <view class="_view data-v-8e3aa6a4 coupon-teltes">
            <view class="_view data-v-8e3aa6a4 coupon-shows">
              <view class="_view data-v-8e3aa6a4 coupon-left" bindtap="handleProxy" data-code="{{item.code}}" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}">
                <view class="_view data-v-8e3aa6a4 left-money">
                  <text class="_text data-v-8e3aa6a4">{{item.type}}</text>
                </view>
                <view class="_view data-v-8e3aa6a4 left-telte">
                  <text class="_text data-v-8e3aa6a4">{{item.use_rule}}</text>
                </view>
              </view>
              <view class="_view data-v-8e3aa6a4 coupon-right" bindtap="handleProxy" data-code="{{item.code}}" data-eventid="{{'1-'+index}}" data-comkey="{{$k}}">
                <text class="_text data-v-8e3aa6a4 right-money">¥</text>
                <text class="_text data-v-8e3aa6a4 right-nums">{{item.face_value}}</text>
              </view>
            </view>
          </view>
          <view class="_view data-v-8e3aa6a4 time">使用期限 : {{item.begin_date}}--{{item.end_date}}</view>
        </view>
      </view>
      <view class="_p data-v-8e3aa6a4 loadmore" bindtap="handleProxy" wx:if="{{flag=='nosucc'}}" data-eventid="{{'2'}}" data-comkey="{{$k}}">
        <label wx:if="{{coupons=='undefined' || coupons.length==0}}" class="_span data-v-8e3aa6a4"></label>
        <label wx:else class="_span data-v-8e3aa6a4">{{loadmore1}}</label>
      </view>
      <view class="_p data-v-8e3aa6a4 loadmore" wx:else>
        <label wx:if="{{coupons=='undefined' || coupons.length==0}}" class="_span data-v-8e3aa6a4"></label>
        <label wx:else class="_span data-v-8e3aa6a4">{{loadmore1}}</label>
      </view>
    </view>
  </view>
</template>